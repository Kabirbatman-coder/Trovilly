          "colorApproach": "Gradient-heavy with soft pastels, rainbow accent gradients",
          "personality": "Warm, approachable, optimistic, personal"
        },
        "layoutPatterns": {
          "alignment": "Center-aligned for hero, mixed for content sections",
          "whitespace": "Very generous, breathing room emphasized",
          "hierarchy": "Strong through size contrast and weight",
          "gridStructure": "Loose and organic, asymmetric photo placement"
        },
        "typographyPatterns": {
          "headlineStyle": "Extra large, bold, tight line-height",
          "accentWords": "Italic with gradient color treatment",
          "bodyText": "Medium gray, generous line-height for readability",
          "contrast": "Dark headlines on light backgrounds with subtle gradient overlays"
        },
        "interactionPatterns": {
          "hoverEffects": [
            "Subtle lift (translateY)",
            "Shadow expansion",
            "Color transitions",
            "Rotation normalization on photos"
          ],
          "buttonStyle": "Pill-shaped with gradient fills",
          "feedbackStyle": "Smooth, spring-based animations",
          "microInteractions": "Gentle, playful, physics-based"
        },
        "photoTreatment": {
          "style": "Polaroid-like with white borders",
          "rotation": "Slight rotation for organic feel (-3deg)",
          "shadow": "Strong shadow for depth",
          "corners": "Large border-radius (1.5rem)",
          "border": "Thick white border (8px)",
          "hover": "Straighten and slight scale"
        },
        "responsiveApproach": {
          "strategy": "Mobile-first with emphasis on touch targets",
          "breakpointBehavior": "Fluid with graceful degradation",
          "navigationPattern": "Always-visible on desktop, likely hamburger on mobile",
          "contentReflow": "Stack vertically on mobile, asymmetric grid on desktop"
        },
        "brandElements": {
          "gradients": "Rainbow multi-stop gradients (pink→orange→yellow→green→blue)",
          "accentColor": "Rainbow gradient as primary brand element",
          "backgroundTexture": "Subtle pastel gradient overlay across entire page",
          "iconography": "Rounded square with gradient fill for logo",
          "newBadge": "Bright pink/red 'NEW!' badge with uppercase styling"
        }
      },
      
      "usageGuidelines": {
        "dos": [
          "Use pill-shaped buttons (border-radius: 9999px) for all CTAs",
          "Apply rainbow gradients to primary actions and accent text",
          "Rotate photo cards slightly (-3deg) for organic, scrapbook feel",
          "Use generous whitespace and breathing room between sections",
          "Apply glassmorphism effect to floating elements (navbar, cards)",
          "Use italic styling with gradient for emphasized words in headlines",
          "Maintain soft, pastel background gradients for warmth",
          "Use thick white borders on photo cards for Polaroid effect",
          "Apply smooth spring-based animations (cubic-bezier with overshoot)"
        ],
        "donts": [
          "Don't use sharp corners on buttons or primary UI elements",
          "Don't use single solid colors for primary CTAs - always use gradients",
          "Don't place photos perfectly straight - add subtle rotation",
          "Don't use harsh shadows - keep them soft and subtle",
          "Don't crowd elements - maintain generous spacing",
          "Don't use pure black text - use dark grays (#111827, #1F2937)",
          "Don't forget backdrop-filter blur on glassmorphic elements",
          "Don't use standard hover effects - add playful transforms"
        ],
        "keyPatterns": [
          "Hero sections: Large centered text with gradient accent words",
          "Photos: White border + rotation + shadow for scrapbook aesthetic",
          "Buttons: Gradient fill + pill shape + lift on hover",
          "Navigation: Glassmorphic fixed navbar with blur",
          "Backgrounds: Subtle multi-color pastel gradients",
          "Input fields: Pill-shaped with subtle shadows",
          "Typography: System fonts, bold headlines, italic accents with gradients"
        ]
      }
    }
