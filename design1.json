      "layout": {
        "grid": {
          "columns": 12,
          "gap": "2rem",
          "breakpoints": {
            "mobile": "640px",
            "tablet": "768px",
            "desktop": "1024px",
            "wide": "1280px",
            "ultraWide": "1536px"
          }
        },
        "container": {
          "maxWidth": "1280px",
          "padding": "0 1.5rem",
          "centered": true,
          "margin": "0 auto"
        },
        "hero": {
          "paddingTop": "8rem",
          "paddingBottom": "6rem",
          "minHeight": "90vh",
          "display": "flex",
          "alignItems": "center",
          "justifyContent": "center"
        },
        "sections": {
          "paddingY": "5rem",
          "paddingX": "1.5rem",
          "spacing": "8rem"
        }
      },
      
      "animations": {
        "transitions": {
          "fast": "150ms",
          "base": "200ms",
          "medium": "300ms",
          "slow": "400ms",
          "slower": "600ms"
        },
        "easings": {
          "easeIn": "cubic-bezier(0.4, 0, 1, 1)",
          "easeOut": "cubic-bezier(0, 0, 0.2, 1)",
          "easeInOut": "cubic-bezier(0.4, 0, 0.2, 1)",
          "spring": "cubic-bezier(0.34, 1.56, 0.64, 1)"
        },
        "commonAnimations": {
          "fadeIn": {
            "keyframes": "opacity: 0 → 1",
            "duration": "400ms",
            "easing": "ease-out"
          },
          "slideUp": {
            "keyframes": "transform: translateY(30px) → translateY(0), opacity: 0 → 1",
            "duration": "500ms",
            "easing": "cubic-bezier(0.34, 1.56, 0.64, 1)"
          },
          "scaleIn": {
            "keyframes": "transform: scale(0.95) → scale(1), opacity: 0 → 1",
            "duration": "400ms",
            "easing": "ease-out"
          },
          "floatPhoto": {
            "keyframes": "transform: rotate(-3deg) translateY(0) → rotate(-3deg) translateY(-10px)",
            "duration": "3000ms",
            "easing": "ease-in-out",
            "iterationCount": "infinite",
            "direction": "alternate"
          }
        }
      },
      
      "effects": {
        "glassmorphism": {
          "background": "rgba(255, 255, 255, 0.7)",
          "backdropFilter": "blur(12px)",
          "WebkitBackdropFilter": "blur(12px)",
          "border": "1px solid rgba(255, 255, 255, 0.8)",
          "boxShadow": "0 8px 32px rgba(0, 0, 0, 0.08)"
        },
        "gradientText": {
          "background": "linear-gradient(135deg, #FF6B9D, #FFA06B, #4D9EFF)",
          "WebkitBackgroundClip": "text",
          "backgroundClip": "text",
          "WebkitTextFillColor": "transparent"
        },
        "softGlow": {
          "boxShadow": "0 0 40px rgba(99, 102, 241, 0.15)"
        }
      },
      
